{
  "name": "@hostconfig/static",
  "version": "0.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "type": "module",
  "readme": "https://github.com/hostconfig/static#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hostconfig/static.git"
  },
  "packageManager": "yarn@1.22.19",
  "scripts": {
    "build": "tsc -p ./tsconfig.json && tsc-alias && ts-add-js-extension --dir=dist",
    "postbuild": "cp -rv ./src/www ./dist/www",
    "rebuild": "yarn clean && yarn build",
    "clean": "rm -rvf ./dist",
    "wipe": "yarn clean && rm -rvf ./node_modules"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "chai": "^4.3.10",
    "chai-http": "^4.4.0",
    "cross-env": "^7.0.3",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "http": "^0.0.1-security",
    "http-errors": "^2.0.0",
    "morgan": "^1.10.0",
    "pug": "^3.0.2",
    "rimraf": "^5.0.5",
    "ts-add-js-extension": "^1.6.0",
    "tsc-alias": "^1.8.8"
  }
}
